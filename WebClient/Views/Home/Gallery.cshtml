@model Muslim.Core.Domain.Thumbnails.Thumbnail

<div class="col-xs-12 col-sm-4 col-md-3">
    <div class="recent-work-wrap">
        <img class="img-responsive" src="@Model.Image" alt="">
        <div class="overlay">
            <div class="recent-work-inner">
                <h3><a href="#">@Model.Title</a> </h3>
                <p>@Model.Description</p>
                <a class="preview" href="@Model.Image" rel="prettyPhoto"><i class="fa fa-eye"></i> View</a>
            </div>
        </div>
    </div>
</div>